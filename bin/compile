#!/usr/bin/env bash

set -e

indent() {
    sed -u 's/^/      /'
}

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3

ls -la
echo '--'
ls -la ${BUILD_DIR}
${BUILD_DIR}/init_script/heroku_deploy.py

if ! [ $? ]; then
    echo "init_script/heroku_deploy.py FAILED" | indent
    exit 1
fi

echo "init_script/heroku_deploy.py finished successfully" | indent
